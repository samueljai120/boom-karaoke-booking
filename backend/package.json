{
  "name": "boom-karaoke-backend",
  "version": "1.0.0",
  "description": "Backend API for Boom Karaoke booking system",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server-minimal-railway.js",
    "dev": "nodemon server.js",
    "build": "tsc",
    "dev:ts": "ts-node server.ts",
    "init-db": "node scripts/init-db.js",
    "setup-postgres": "node scripts/setup-postgres.js",
    "migrate:up": "node scripts/migrate-sqlite-to-postgres.js",
    "migrate:railway": "node scripts/migrate-to-railway.js",
    "migrate:down": "node scripts/migrate-down.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:reset": "npm run migrate:down && npm run migrate:up",
    "health": "node -e \"require('http').get('http://localhost:5000/api/health', (res) => { console.log('Health check:', res.statusCode); process.exit(res.statusCode === 200 ? 0 : 1) })\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "socket.io-redis": "^6.1.1",
    "sqlite3": "^5.1.6",
    "stripe": "^18.5.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "karaoke",
    "booking",
    "api",
    "express",
    "sqlite"
  ],
  "author": "Boom Karaoke Team",
  "license": "MIT"
}
